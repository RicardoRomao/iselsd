<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<add key="DataSource" value="./rep.xml" />
	</appSettings>
	<system.runtime.remoting>
		<application>
			<channels>
				<channel ref="tcp" port="0">
					<clientProviders>
						<formatter ref="binary" />
					</clientProviders>
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
					</serverProviders>
				</channel>
			</channels>
			<client>
				<wellknown type="Proxy.IZoneServer, Proxy" url="tcp://localhost:9001/RemoteServer.rem" />
			</client>
		</application>
	</system.runtime.remoting>
</configuration>